            };
            // With JQuery
            $("#ex13").slider({
                tooltip: 'hide',
                step: 10,
                ticks: [10, 20, 30, 40],
                ticks_labels: ['UberBlack', 'uberX', 'uberVan', 'uberTaxi'],
                value: 20
            }).on('change', function (obj) {
                console.log(obj.value);
